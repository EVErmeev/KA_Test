	|				* ЕСТЬNULL(ИтогиБазыПоПодразделениям.СтоимостьБезНДС, 
	|					ЕСТЬNULL(ИтогиБазыПоЭтапам.СтоимостьБезНДС, 1)) КАК ЧИСЛО(28, 10))
	|	КОНЕЦ																	КАК ДоляСтоимостиБезНДС,
