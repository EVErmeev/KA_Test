Процедура УчетВнеоборотныхАктивовПриИзменении(Форма, Отказ) Экспорт

	//++ Локализация
	
	Если Форма.УчетВнеоборотныхАктивов = "2_2" И НЕ Форма.ДоступностьУчета22.ДоступенУчет Тогда
		
		ПоказатьПредупреждение(, Форма.ДоступностьУчета22.КомментарийУчет2_2);
		
		Отказ = Истина;
		
	ИначеЕсли Форма.УчетВнеоборотныхАктивов = "2_2" И Форма.НаличиеУчета.ИспользуетсяУУНаПланеСчетовХозрасчетный Тогда
		
		ТекстСообщения = НСтр("ru = 'Для включения учета версии 2.2 необходимо выключить опцию ""Управленческий учет на плане счетов регламентированного учета"" (раздел ""Регламентированный учет"" - ""Отражение операций"")'");
		
		ПоказатьПредупреждение(,ТекстСообщения); 
		
		Отказ = Истина;
		
	ИначеЕсли Форма.УчетВнеоборотныхАктивов = "2_4" 
		И Форма.НаборКонстант.ИспользоватьВнеоборотныеАктивы2_2
		И НЕ Форма.НаборКонстант.ИспользоватьВнеоборотныеАктивы2_4 Тогда
		
		ОткрытьФорму("Обработка.ПомощникПереходаНаУчетВнеоборотныхАктивовВерсии24.Форма");
		
		Отказ = Истина;
		
	ИначеЕсли Форма.УчетВнеоборотныхАктивов = ""
		И Форма.НаличиеУчета.ИспользоватьРеглУчет Тогда
		
		ТекстСообщения = НСтр("ru = 'Для отключения учета необходимо выключить опцию ""Регламентированный учет"" (раздел ""Регламентированный учет"")'");
		
		ПоказатьПредупреждение(,ТекстСообщения); 
		
		Отказ = Истина;
		
	КонецЕсли;
	
	//-- Локализация
	
КонецПроцедуры

Процедура ПриПониженииВерсииУчетаВнеоборотныхАктивов(Форма, ТекстВопроса) Экспорт

	//++ Локализация
	
	Если Форма.УчетВнеоборотныхАктивов = ""
		И Форма.НаборКонстант.ИспользоватьВнеоборотныеАктивы2_2 Тогда
			
