	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>~Список.Документ</Field>
				<Field>Список.БЗК_ДокументСВерсиейДляПечати</Field>
				<Field>Список.Внешний</Field>
				<Field>Список.КатегорияДокумента</Field>
				<Field>Список.КраткийСоставДокумента</Field>
				<Field>Список.ОжидаетПодписания</Field>
				<Field>Список.ОснованиеДокумента</Field>
				<Field>Список.Расширение</Field>
				<Field>Список.Ссылка</Field>
				<Field>Список.ТребуетВнимания</Field>
				<Field>Список.ЭлектронныйДокумент</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДокументДокументКадровогоЭДО.Ссылка КАК Ссылка,
	ДокументДокументКадровогоЭДО.ПометкаУдаления КАК ПометкаУдаления,
	ДокументДокументКадровогоЭДО.Номер КАК Номер,
	ДокументДокументКадровогоЭДО.Дата КАК Дата,
	ДокументДокументКадровогоЭДО.Проведен КАК Проведен,
	ДокументДокументКадровогоЭДО.Организация КАК Организация,
	ДокументДокументКадровогоЭДО.ЭлектронныйДокумент КАК ЭлектронныйДокумент,
	ДокументДокументКадровогоЭДО.ОснованиеДокумента КАК ОснованиеДокумента,
	ДокументДокументКадровогоЭДО.ИдентификаторЗаявкиКабинетСотрудника КАК ИдентификаторЗаявкиКабинетСотрудника,
	ДокументДокументКадровогоЭДО.ИдентификаторДокумента КАК ИдентификаторДокумента,
	ДокументДокументКадровогоЭДО.Комментарий КАК Комментарий,
	ДокументДокументКадровогоЭДО.МоментВремени КАК МоментВремени,
	ДокументДокументКадровогоЭДО.КатегорияДокумента КАК КатегорияДокумента,
	ВЫРАЗИТЬ("" КАК СТРОКА(100)) КАК ПодписанСотрудниками,
	ДокументДокументКадровогоЭДО.КраткийСоставДокумента КАК КраткийСоставДокумента,
	ЕСТЬNULL(РасчетныеЛистки.Месяц, ДАТАВРЕМЯ(1, 1, 1)) КАК Период,
	ЕСТЬNULL(РасчетныеЛистки.ДатаПубликации, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаПубликации,
	ЛОЖЬ КАК ОжидаетПодписания,
	ЛОЖЬ КАК ТребуетВнимания,
	ВЫБОР
		КОГДА ДокументДокументКадровогоЭДО.КатегорияДокумента = ЗНАЧЕНИЕ(Перечисление.КатегорииДокументовКадровогоЭДО.РасчетныйЛисток)
				ИЛИ ДокументДокументКадровогоЭДО.КатегорияДокумента = ЗНАЧЕНИЕ(Перечисление.КатегорииДокументовКадровогоЭДО.ЗаявлениеСотрудника)
			ТОГДА ДокументДокументКадровогоЭДО.КатегорияДокумента
		ИНАЧЕ ТИПЗНАЧЕНИЯ(ДокументДокументКадровогоЭДО.ОснованиеДокумента)
	КОНЕЦ КАК ВидОснования,
	ЛОЖЬ КАК БЗК_ДокументСВерсиейДляПечати,
	ДокументДокументКадровогоЭДО.Внешний КАК Внешний,
	"          " КАК Расширение
ИЗ
	Документ.ДокументКадровогоЭДО КАК ДокументДокументКадровогоЭДО
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.РасчетныеЛисткиКабинетСотрудника КАК РасчетныеЛистки
		ПО ДокументДокументКадровогоЭДО.Ссылка = РасчетныеЛистки.ДокументКадровогоЭДО}
{ГДЕ
	(ДокументДокументКадровогоЭДО.ЭлектронныйДокумент В
			(ВЫБРАТЬ
				ЗапланированныеДействия.ПрисоединенныйФайл
			ИЗ
				РегистрСведений.ЗапланированныеДействияСФайламиДокументовКЭДО КАК ЗапланированныеДействия
			ГДЕ
				ДокументДокументКадровогоЭДО.ЭлектронныйДокумент = ЗапланированныеДействия.ПрисоединенныйФайл
				И ЗапланированныеДействия.Исполнитель В (&amp;Исполнители)
				И ЗапланированныеДействия.Действие = ЗНАЧЕНИЕ(Перечисление.ДействияСФайламиДокументовКЭДО.Подписать))),
	(ДокументДокументКадровогоЭДО.ЭлектронныйДокумент В
			(ВЫБРАТЬ
				ПодписанныеОбъектыПользователей.ПодписанныйОбъект
			ИЗ
				РегистрСведений.ПодписанныеОбъектыПользователей КАК ПодписанныеОбъектыПользователей
			ГДЕ
				ДокументДокументКадровогоЭДО.ЭлектронныйДокумент = ПодписанныеОбъектыПользователей.ПодписанныйОбъект
				И ПодписанныеОбъектыПользователей.УстановившийПодпись В (&amp;Подписанты))),
	(ДокументДокументКадровогоЭДО.СодержимоеДокумента В (&amp;СодержимоеДокументов)),
	(ИСТИНА В
			(ВЫБРАТЬ ПЕРВЫЕ 1
